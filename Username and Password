#include<iostream>
#include<cmath>
using namespace std;
int main()
{

		string username = "Christiffany Casas";
		string password = "123456789";

		string user, pass;

		cout<<"Type your Username: ";
		cin>>user;

		cout<<"Type your Password: ";
		cin>>pass;

		if (username=="Christiffany Casas"&&password=="123456789"){
	
			cout<<"Same";
		}
		else{
			cout<<"Not Same";
		}


		return 0;
	
}
